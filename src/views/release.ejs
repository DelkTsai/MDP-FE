<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>发布组件</title>
  <% include ../layout/header.ejs %>
</head>
<body>
<div id="app">
  <h1>发布组件</h1>
  <div>
    <label for="">选择组件</label>
    <select v-model="selected">
      <option v-for="option in options" v-bind:value="option.value">
        {{ option.text }}
      </option>
    </select>
    <i class="material-icons" v-on:click="addComponent">add_circle</i>
    <div>
      <ul>
        <li v-for="item in items">
          <span v-text="item.message"></span>
        </li>
      </ul>
    </div>
  </div>

</div>

<script src="/js/release.js"></script>
</body>
</html>